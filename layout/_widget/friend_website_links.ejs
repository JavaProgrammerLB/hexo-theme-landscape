<% if (site.posts.length){ %>
  <div class="widget-wrap">
    <h3 class="widget-title">
      <%= __('friend_website_links') %>
    </h3>
    <div class="widget">
      <ul>
        <% config.friend_website_links.each(function(link){ %>
          <li>
            <a href="<%- link.url %>"><%= link.name %></a>
          </li>
        <% }) %>
      </ul>
    </div>
  </div>
  <% } %>